{
  "dashboard": {
    "id": null,
    "title": "Carbon Capture Network Overview",
    "tags": ["carbon-capture", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"carbon-capture-backend\"}",
            "legendFormat": "Backend",
            "refId": "A"
          },
          {
            "expr": "up{job=\"carbon-capture-ai-engine\"}",
            "legendFormat": "AI Engine",
            "refId": "B"
          },
          {
            "expr": "up{job=\"mongodb\"}",
            "legendFormat": "MongoDB",
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"carbon-capture-backend\"}[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "AI Engine Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_predictions_total[5m])",
            "legendFormat": "Predictions/min",
            "refId": "A"
          },
          {
            "expr": "ai_prediction_duration_seconds{quantile=\"0.95\"}",
            "legendFormat": "95th percentile latency",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "mongodb_connections{state=\"active\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections{state=\"available\"}",
            "legendFormat": "Available Connections",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Sensor Data Volume",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sensor_readings_total[5m])",
            "legendFormat": "Readings/sec",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Carbon Capture Efficiency",
        "type": "graph",
        "targets": [
          {
            "expr": "carbon_capture_efficiency_percent",
            "legendFormat": "Efficiency %",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "5xx Error Rate %",
            "refId": "A"
          },
          {
            "expr": "rate(sensor_readings_failed_total[5m]) / rate(sensor_readings_total[5m]) * 100",
            "legendFormat": "Sensor Error Rate %",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
